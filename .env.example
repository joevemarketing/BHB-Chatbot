# OpenAI-compatible LLM (text + optional vision)
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL_NAME=gpt-4o-mini
OPENAI_VISION_MODEL=gpt-4o-mini
# Optional: set a custom base URL for OpenAI-compatible providers
# OPENAI_BASE_URL=https://api.openai.com/v1

# Optional Gemini Vision integration
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-1.5-flash

# Vision provider selection: openai or gemini
VISION_PROVIDER=openai

# Optional WooCommerce integration (BHB.com.my)
WC_API_URL=https://www.bhb.com.my
WC_CONSUMER_KEY=your_wc_consumer_key
WC_CONSUMER_SECRET=your_wc_consumer_secret

# Admin panel passcode (protects /admin and KB uploads)
ADMIN_PASSCODE=admin123